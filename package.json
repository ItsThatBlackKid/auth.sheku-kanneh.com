{
  "name": "auth",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start-build": "node ./build/bin/www",
    "clean": "rm -rf ./build",
    "prebuild": "npm run clean",
    "build": "babel ./bin/** ./src/*.js -d ./build --ignore node_modules, auth-ui",
    "start": "nodemon ./bin/www --ignore 'auth-ui' --exec babel-node -e js"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.16",
    "bcrypt": "^3.0.7",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-cors": "0.0.3",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.1",
    "morgan": "~1.9.1",
    "node-rsa": "^1.0.7",
    "node-sass-middleware": "0.11.0",
    "nodemon": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1"
  }
}
